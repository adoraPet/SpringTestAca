  <mapper namespace="io.adorapet.ex.jsp.dao.SellerDAO">

	<insert id="insertUser" parameterType="map">
		INSERT INTO
			`seller`
		(
			`nickname`
			, `profileImage`
			, `temperature`
			, `createdAt`
			, `updatedAt`
		) 
		VALUE 
		(
			#{nickname}
			, #{profileImage}
			, #{temperature}
			, now()
			, now()
		)
	
	</insert>
</mapper>